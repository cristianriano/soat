<% flash.each do |type, msg| %>
  $('#alerts').html($('#alerts').html() + "<div class='alert alert-<%= type %>' role='alert'><%= msg %></div>");
  $('#new_vehicle_form_container').html("<%= j render partial: 'vehicles/new_form' %>");
<% end %>
<% if @vehicle.persisted? %>
  $("#vehicle_section").hide();
  <%= render partial: 'vehicles/enable_insurance_form' %>
<% end %>
